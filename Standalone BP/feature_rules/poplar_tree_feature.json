{
  "format_version": "1.13.0",
  "minecraft:feature_rules": {
    "description": {
      "identifier": "terra:poplar_tree_feature",
      "places_feature": "terra:poplar_tree_feature"
    },
    "conditions": {
      "placement_pass": "surface_pass",
      "minecraft:biome_filter": [
        {
          "all_of": [
            {
              "test": "has_biome_tag",
              "operator": "!=",
              "value": "ocean"
            },
            {
              "test": "has_biome_tag",
              "operator": "!=",
              "value": "river"
            },
            {
              "any_of": [
                {
                  "test": "has_biome_tag",
                  "operator": "==",
                  "value": "swamp"
                },
                {
                  "test": "has_biome_tag",
                  "operator": "==",
                  "value": "taiga"
                },
                {
                  "test": "has_biome_tag",
                  "operator": "==",
                  "value": "roofed"
                },
                {
                  "test": "has_biome_tag",
                  "operator": "==",
                  "value": "extreme_hills"
                },
                {
                  "test": "has_biome_tag",
                  "operator": "==",
                  "value": "forest"
                }
              ]
            }
          ]
        }
      ]
    },
    "distribution": {
      "iterations": 3,
      "x": {
        "distribution": "uniform",
        "extent": [
          0,
          16
        ]
      },
      "y": "query.heightmap(variable.worldx, variable.worldz) + -1",
      "z": {
        "distribution": "uniform",
        "extent": [
          0,
          16
        ]
      }
    }
  }
}