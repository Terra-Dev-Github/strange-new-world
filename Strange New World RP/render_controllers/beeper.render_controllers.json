{
    "format_version": "1.8.0",
    "render_controllers": {
        "controller.render.beeper": {
            "geometry": "Geometry.default",
            "materials": [
                {
                    "crystal": "Material.outline"
                },
                {
                    "*": "Material.default"
                }
            ],
            "textures": [
                "q.is_shaking ? Texture.beeper_red : Texture.default"
            ],
            "part_visibility": [
                {
                    "*": true
                },
                {
                    "crystal": false
                }
            ],
            "ignore_lighting": false
        },
        "controller.render.beeper_outline": {
            "geometry": "Geometry.default",
            "materials": [
                {
                    "crystal": "Material.outline"
                },
                {
                    "*": "Material.default"
                }
            ],
            "textures": [
                "q.is_shaking ? Texture.beeper_red : Texture.default"
            ],
            "part_visibility": [
                {
                    "*": false
                },
                {
                    "crystal": true
                }
            ],
            "ignore_lighting": true
        }
    }
}