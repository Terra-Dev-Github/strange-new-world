{
    "format_version": "1.8.0",
    "render_controllers": {
        "controller.render.outsider_armor": {
            "geometry": "Geometry.default",
            "materials": [
                {
                    "*": "variable.is_enchanted ? Material.enchanted : Material.default"
                }
            ],
            "textures": [
                "Texture.default",
                "Texture.enchanted"
            ]
        },
        "controller.render.outsider_armor_emissive": {
            "geometry": "Geometry.default",
            "materials": [
                {
                    "*": "Material.emissive"
                }
            ],
            "textures": [
                "Texture.emissive"
            ]
        }
    }
}